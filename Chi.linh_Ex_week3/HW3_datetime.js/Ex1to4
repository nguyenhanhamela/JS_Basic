//Bài 1.Tạo 1 đối tượng Date, in ra terminal ngày tháng năm hiện tại 
var date = new Date();

console.log(date);


//Bài2. Từ bài 1, lấy ra năm hiện tại (4 chữ số) 
console.log(date.getFullYear());

//Bài3. Từ bài 1, lấy ra tháng hiện tại (2 chữ số) 
var month = String(date.getMonth()+1).padStart(2,"0");

console.log(month);


//Bài 4. Từ bài 1, lấy ra ngày hiện tại (2 chữ số) 
var day = String(date.getDate()).padStart(2,"0");

console.log(day);




